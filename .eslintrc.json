{
  "extends": "airbnb-base",
  "plugins": [],
  "rules": {
    "strict": 0,
    "import/no-extraneous-dependencies" : 0,
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "ignore"
    }],
    "no-underscore-dangle": ["error", {
      "allow": ["__"]
    }],
    "camelcase": 0
  },
  "env": {
    "node": true,
    "mocha": true
  }
}